  <Target Name="CopySatellitesToI18n" AfterTargets="Build">
    <!-- 例: obj\Debug\ja\Kiritori.resources.dll, obj\Release\fr\Kiritori.resources.dll ... -->
    <ItemGroup>
      <_SatDlls Include="$(IntermediateOutputPath)**\$(AssemblyName).resources.dll" />
    </ItemGroup>
    <!-- 出力先のフォルダ作成 (bin\...\i18n\ja など) -->
    <MakeDir Directories="@(_SatDlls->'$(TargetDir)i18n\%(RecursiveDir)')" />
    <!-- i18n\{cul}\Kiritori.resources.dll へコピー -->
    <Copy SourceFiles="@(_SatDlls)" DestinationFiles="@(_SatDlls->'i18n\%(RecursiveDir)$(AssemblyName).resources.dll')" SkipUnchangedFiles="true" />
    <!-- ログ（任意） -->
    <Message Text="i18n へコピー: @(_SatDlls->'%(RecursiveDir)$(AssemblyName).resources.dll')" Importance="High" />
  </Target>
  <ItemGroup>
    <!-- 例: ja を埋め込む -->
    <EmbeddedResource Include="i18n\ja\Kiritori.resources.dll">
      <LogicalName>Kiritori.i18n.ja.Kiritori.resources.dll</LogicalName>
    </EmbeddedResource>
    <!-- 他言語も同様に LogicalName を設定 -->
  </ItemGroup>
